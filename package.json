    "@crawlee/playwright": "^3.12.2",
    "cheerio": "^1.0.0",
    "crawlee": "^3.12.2",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.7",
    "npm-check-updates": "^17.1.14",
    "playwright": "^1.50.1",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
