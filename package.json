{
  "name": "omio-scrapper",
  "version": "0.1.0",
  "description": "The Omio Scraper is a webscraper for omio.com",
  "main": "index.ts",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "test": "npx jest --passWithNoTests",
